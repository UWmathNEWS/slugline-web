{
  "name": "slugline-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@types/draft-js": "^0.10.38",
    "@types/jest": "^24.9.1",
    "@types/js-cookie": "^2.2.4",
    "@types/katex": "^0.11.0",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^12.12.25",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^6.0.0",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.3",
    "axios": "^0.19.2",
    "babel-plugin-transform-assets": "^1.0.2",
    "bootstrap": "^4.4.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.19.1",
    "ignore-styles": "^5.0.1",
    "is-hotkey": "^0.1.6",
    "js-cookie": "^2.2.1",
    "katex": "^0.11.1",
    "morgan": "^1.10.0",
    "nanoid": "^2.1.11",
    "node-sass": "^4.14.1",
    "popper.js": "^1.16.0",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "react-hook-form": "^5.6.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "serialize-javascript": "^3.1.0",
    "slate": "^0.57.1",
    "slate-react": "^0.57.1",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && node scripts/generate-manifest.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lib": "npm run app:lib && npm run ssr:lib",
    "app:lib": "rm -rf lib/ && NODE_ENV=production babel --extensions .tsx,.ts --ignore 'src/**/*.test.js' --copy-files src -d lib && npm run app:lib:esm",
    "app:lib:esm": "find lib/ -type f -name *.js -exec sed -i'' -e 's|@babel/runtime/helpers/esm|@babel/runtime/helpers|g' {} +",
    "ssr:lib": "rm -rf server/lib/ && babel --extensions .tsx,.ts -d server/lib server/src && npm run ssr:lib:sed",
    "ssr:lib:sed": "find server/lib/ -type f -name *.js -exec sed -i'' -e 's|/src/|/lib/|g' {} +",
    "start-ssr": "node server/index.js"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-modules-commonjs",
      [
        "transform-assets",
        {
          "extensions": [
            "css",
            "svg"
          ],
          "name": "static/[name].[hash:8].[ext]"
        }
      ]
    ],
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8000",
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^7.2.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/morgan": "^1.9.0",
    "core-js": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "jest-mock-axios": "^4.0.0",
    "mutationobserver-shim": "^0.3.5",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.12.0"
  }
}
